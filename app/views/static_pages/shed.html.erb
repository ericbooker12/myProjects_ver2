		</div>
	</div><!-- /.row -->

	<div class="row">

		<div class="col-md-3 col-xs-3">
			<figure>
				<%= image_tag("shed/shed_before.jpeg", class: "img-responsive", alt: "Annadel Model")%>
				<figcaption>The before shot.</figcaption>
			</figure>
		</div>

		<div class="col-md-3 col-xs-3">
			<figure>
				<%= image_tag("shed/shed_mid_1.jpeg", class: "img-responsive", alt: "Annadel Model")%>
